@model CourseDesign.Models.Engineering
@{
    ViewBag.Title = "GetData";
}

<h2>Excel表中的数据如下</h2>

<body>
    @using (Html.BeginForm())
    {
        <table>
            <tr>
                <td>编号</td>
                <td>姓名</td>
                <td>性别</td>
                <td>学历</td>
                <td>籍贯</td>
                <td>地址</td>
                <td>电话</td>
                <td>工龄</td>
                <td>工资</td>
                <td>出生日期</td>
            </tr>
            @foreach (var a in ViewData["UpList"] as List<CourseDesign.Models.Engineering>)
            {
                <tr>
                    <td>@a.Id</td>
                    <td>@a.Name</td>
                    <td>@a.Sex</td>
                    <td>@a.Education</td>
                    <td>@a.Place</td>
                    <td>@a.Address</td>
                    <td>@a.Telephone</td>
                    <td>@a.Workage</td>
                    <td>@a.Salary</td>
                    <td>@a.Birth</td>


                </tr>
            }
        </table>
    }
    @Html.ActionLink("添加到数据库", "InsertExcelInfo", "Home")
    &nbsp; &nbsp; &nbsp;
    @Html.ActionLink("取消", "UpLoadData", "Home")
</body>
    <br />
    <text>性别&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;</text>@Html.TextBoxFor(model => model.sex)
    <br />
    <br />
    <text>学历&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;</text>@Html.TextBoxFor(model => model.education)
    <br />
    <br />
    <text>籍贯&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;</text>@Html.TextBoxFor(model => model.place)

@model engineering
@{
    ViewBag.Title = "Modify";
}

<h2>修改信息</h2>
@using (Html.BeginForm("Modify", "Home", FormMethod.Post))
{

    <text>编号&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp; &nbsp;</text>@Html.TextBoxFor(model => model.id)
    <br />
    <br />
    <text>姓名&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp; &nbsp;</text>@Html.TextBoxFor(model => model.name)

@model engineering
@{
    ViewBag.Title = "查询结果";
}
<style>
    table, table tr th, table tr td {
        border: 1px solid #0094ff;
    }

    table {
        width: 600px;
        text-align: center;
        border-collapse: collapse;
    }
</style>
@using (Html.BeginForm())
{
    <table>
        <tr>
            <td>编号</td>
            <td>姓名</td>
            <td>性别</td>
            <td>学历</td>
            <td>籍贯</td>
            <td>地址</td>
            <td>电话</td>
            <td>工龄</td>
            <td>工资</td>
            <td>出生日期</td>
        </tr>
        @foreach (var a in ViewData["QueryList"] as List<engineering>)
        {
            <tr>
                <td>@a.id</td>
                <td>@a.name</td>
                <td>@a.sex</td>
                <td>@a.education</td>
                <td>@a.place</td>
                <td>@a.address</td>
                <td>@a.telephone</td>
                <td>@a.workage</td>
                <td>@a.salary</td>
                <td>@a.birth</td>
            </tr>
        }
    </table>
    <br />
    <br />
    <br />
    <br />
    @Html.ActionLink("返回", "QueryData", "Home")
}

